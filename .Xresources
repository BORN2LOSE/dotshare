!Xterm
xterm*VT100.geometry: 69x19
xterm*faceName: Terminus:size=13:antialias=false


! urxvt dot
urxvt.font:xft:Terminus:size=13:autohint=false:hinting=true:style=regular
urxvt.scrollBar: false
urxvt.url-select.launcher : firefox
urxvt.geometry: 71x19
urxvt.perl-ext-common:fullscreen
urxvt.keysym.F11: perl:fullscreen:switch

! Other ----
urxvt.depth: 32
urxvt.loginShell: true

! each theme
urxvt*background: #212121
urxvt*foreground: #43e8d8
urxvt*internalBorder: 20
urxvt*cursorBlink: 1

! Common
#define S_yellow        #b58900
#define S_orange        #cb4b16
#define S_red           #dc322f
#define S_magenta       #d33682
#define S_violet        #6c71c4
#define S_blue          #268bd2
#define S_cyan          #2aa198
#define S_green         #859900


! Dark

#define S_base03        #002b36
#define S_base02        #073642
#define S_base01        #586e75
#define S_base00        #657b83
#define S_base0         #839496
#define S_base1         #93a1a1
#define S_base2         #eee8d5
#define S_base3         #fdf6e3


! Light

! #define S_base03        #fdf6e3
